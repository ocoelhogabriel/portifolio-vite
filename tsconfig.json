{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "types": [
      "@types/node"
    ],
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "#/*": ["./*"],
      "content/*": ["./content/*"]
    }
  },
  "include": [
    "types/**/*.d.ts",
    "**/*.ts",
    "**/*.vue"
  ]
}
